<?xml version="1.0" encoding="utf-8"?><FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"             android:layout_width="match_parent"             android:layout_height="match_parent">    <ListView            android:id="@id/android:list"            android:layout_width="match_parent"            android:layout_height="match_parent"            android:layout_weight="1"            android:fastScrollEnabled="true"/>    <TextView            android:id="@id/android:empty"            android:layout_width="match_parent"            android:layout_height="match_parent"            android:text="@string/no_artists_found"            android:textColor="@color/asbestos"            android:textSize="24sp"            android:gravity="center"/></FrameLayout>